{
  "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "sourceMap": true,
      "target": "es5",
      "lib": ["es6", "dom"],
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "experimentalDecorators": true
  },
  "exclude": [
      "node_modules"
  ]
}